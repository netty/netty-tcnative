1000 ICON "apache.ico"

1001 DIALOGEX 0, 0, 252, 51
STYLE DS_SETFONT | DS_MODALFRAME | DS_FIXEDSYS | DS_CENTER | WS_CAPTION
CAPTION "Password prompt"
FONT 8, "MS Shell Dlg", 0, 0, 0x0
BEGIN
    ICON            1000,-1,8,6,21,20
