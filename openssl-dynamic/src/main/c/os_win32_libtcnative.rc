            VALUE "OriginalFilename", "libtcnative-1.dll\0"
            VALUE "ProductName", "Tomcat Native Java Library\0"
            VALUE "ProductVersion", TCN_VERSION "\0"
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x409, 1200
    END
END

